var X, Y, Z;

procedure MCD;
var F,G;
begin
  F := X; G := Y;
  while F <> G do
    begin
      if F < G then  G := G - F;
      IF G < F then  F := F - G
    end;
  Z:= F
end;

begin
  write ('X: '); readln (X);
  if X > 0 then
    begin
      write ('Y: '); readln (Y);
      if Y > 0 then
        begin
          call MCD;
          writeln ('MCD: ', Z); writeln
        end
    end
end.
